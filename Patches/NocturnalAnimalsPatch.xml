<?xml version="1.0" encoding="utf-8" ?>
<Patch>

     <!-- Wooly Duskling -->
    <Operation Class="PatchOperationAddModExtension">
        <xpath>/Defs/ThingDef[defName="FO_WoolyDuskling"]</xpath>
        <value>
            <li Class="NocturnalAnimals.ExtendedRaceProperties">
                <bodyClock>Nocturnal</bodyClock>
            </li>
        </value>
    </Operation>
	
	<!-- Dusk Prowler -->
    <Operation Class="PatchOperationAddModExtension">
        <xpath>/Defs/ThingDef[defName="FO_DuskProwler"]</xpath>
        <value>
            <li Class="NocturnalAnimals.ExtendedRaceProperties">
                <bodyClock>Nocturnal</bodyClock>
            </li>
        </value>
    </Operation>
	
	

</Patch>